        let isTabFixed = false;
        Stack.add(function () {
            $(window).scroll(function () {
                if ($('.app-induce-banner').length > 0) {
                    if ($(this).scrollTop() > 56) {
                        if (isTabFixed) {
                            return;
                        }

                        isTabFixed = true;

                        $('.head').addClass('head-fixed');
                    } else {
                        if (!isTabFixed) {
                            return;
                        }

                        isTabFixed = false;

                        $('.head').removeClass('head-fixed');
                    }
                }
            });
        });

